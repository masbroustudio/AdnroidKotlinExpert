<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher_kokas"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_kokas_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KokasSubexpert">

        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.KokasSubexpert.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".detail.DetailKokasActivity"
            android:label="@string/title_activity_detail_home"
            android:theme="@style/Theme.KokasSubexpert.NoActionBar" />

        <activity
            android:name=".about.AboutPageActivity"
            android:exported="false" />
    </application>

</manifest>